{
  "name": "circuit-backend",
  "version": "0.1.0",
  "type": "module",
  "description": "Backend server for Circuit",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch --ignore '../frontend/**' src/index.ts",
    "build": "echo 'Backend uses tsx runtime, no build step needed'",
    "start": "tsx src/index.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:all": "node scripts/test-runner.js",
    "test:quick": "node scripts/test-runner.js --quick",
    "test:workflows": "node scripts/test-runner.js --workflow"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.37",
    "@openai/codex-sdk": "^0.79.0",
    "express": "^4.18.2",
    "js-yaml": "^3.14.2",
    "socket.io": "^4.7.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.2.1",
    "jest": "^29.7.0",
    "socket.io-client": "^4.7.5",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
}
